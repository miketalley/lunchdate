<div class="container">
<h1><%= @profile.username %></h1>
<div class="panel">
  <div class="row collapsed">
    <div class="large-3 columns">
      <%= image_tag(@profile.image_url, size: '200x200') %>
    </div>
    <div class="large-9 columns">
      <label>Quote</label><%= @profile.quote %>
    </div>
  </div>

  <div class="row collapsed">
    <div class="large-3 columns">
      <label>Hometown</label><%= @profile.city %>, <%= @profile.state %>
    </div>
    <div class="large-3 columns">
      <label>Age</label><%= @profile.age(@profile.dob) %>
    </div>
    <div class="large-3 columns">
      <label>Gender</label><%= @profile.gender %>
    </div>
    <div class="large-3 columns">
      <label>Sexual Orientation</label><%= @profile.sexual_orientation %>
    </div>
  </div>

</div>


</div>
